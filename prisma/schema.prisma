generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DATABASE_URL_UNPOOLED")
}

model Collection {
    id Int @id @default(autoincrement())
    mal_id String 
    title String
    image String
    user_email String
    cat String?
    @@unique([user_email, mal_id])
}
